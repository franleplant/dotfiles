	email = franleplant@gmail.com
[core]
	editor = vim
    excludesfiles = '~/.gitignore_global'
	filemode = false
[diff]
    tool = vimdiff


[alias]
  co = checkout
  ci = commit
  st = status
  lg = "log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit"
